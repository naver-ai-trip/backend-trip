APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
FILESYSTEM_PUBLIC_DISK=public
QUEUE_CONNECTION=database

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

# ============================================================================
# Social Login (OAuth) Configuration
# ============================================================================

# Google OAuth
# Get your keys from: https://console.cloud.google.com/apis/credentials
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_REDIRECT_URI="${APP_URL}/auth/google/callback"

# ============================================================================
# NAVER Developers API Keys (openapi.naver.com)
# ============================================================================
# Get your keys from: https://developers.naver.com/apps/#/register
# These APIs use Client ID/Secret authentication with openapi.naver.com domain

# NAVER Local Search API (openapi.naver.com)
# Used for: Place search, business information
NAVER_DEVELOPERS_CLIENT_ID=your_client_id_here
NAVER_DEVELOPERS_CLIENT_SECRET=your_client_secret_here
NAVER_LOCAL_SEARCH_ENABLED=true

# ============================================================================
# NAVER Cloud Platform (NCP) API Keys (ntruss.com)
# ============================================================================
# Get your keys from: https://console.ncloud.com/
# These APIs use different authentication methods (Client ID/Secret or API Gateway Key)

# NAVER Maps API (NCP)
# Used for: POI search, geocoding, reverse geocoding, distance calculation
NAVER_MAPS_CLIENT_ID=your_client_id_here
NAVER_MAPS_CLIENT_SECRET=your_client_secret_here

# NAVER Papago Translation API
# Used for: Text translation, language detection
# Supported languages: ko, en, ja, zh-CN, zh-TW, es, fr, de, ru, pt, it, vi, th, id, ar
NAVER_PAPAGO_CLIENT_ID=your_client_id_here
NAVER_PAPAGO_CLIENT_SECRET=your_client_secret_here

# NAVER Clova OCR (Optical Character Recognition)
# Used for: Extracting text from images (menus, signs, etc.)
NAVER_CLOVA_OCR_URL=https://your-ocr-endpoint.apigw.ntruss.com/custom/v1/your-domain/your-version/document/receipt
NAVER_CLOVA_OCR_SECRET_KEY=your_secret_key_here

# NAVER Clova Speech (STT - Speech to Text)
# Used for: Converting audio to text for translation
NAVER_SPEECH_CLIENT_ID=your_client_id_here
NAVER_SPEECH_CLIENT_SECRET=your_client_secret_here

# NAVER Green-Eye (Content Moderation)
# Used for: Detecting adult content and violence in images
# Get your credentials from: https://console.ncloud.com/ai-service
# Format: https://[your-apigw-id].apigw.ntruss.com/custom/v1/[domain-id]/[secret]/greeneye
NAVER_GREENEYE_URL=https://your-apigw-id.apigw.ntruss.com/custom/v1/your-domain/your-secret/greeneye
NAVER_GREENEYE_SECRET_KEY=your_secret_key_here
NAVER_GREENEYE_ENABLED=true
NAVER_GREENEYE_TIMEOUT=30

# API Rate Limiting & Retry Configuration
NAVER_API_TIMEOUT=30
NAVER_API_RETRY_TIMES=3
NAVER_API_RETRY_SLEEP=1000

# Feature Flags
NAVER_TRANSLATION_ENABLED=true
NAVER_MAPS_ENABLED=true
NAVER_OCR_ENABLED=true
NAVER_SPEECH_ENABLED=true
